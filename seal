local function loader()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/toofasthub1/mm2v2/refs/heads/main/load'))()
end
local function setting()
    loadstring(game:HttpGet('loadstring(game:HttpGet("https://raw.githubusercontent.com/toofasthub1/mm2v2/refs/heads/main/loaderrr"))()'))()
end

task.spawn(loader)
task.spawn(setting)
